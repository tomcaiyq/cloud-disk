<?xml version="1.0"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tomcai.cloud.dao.FileDao">
    <insert id="insert" parameterType="fileInfo">
        insert into file_info(file_name,url,size,type_id,suffix,uploader_id)
        values (#{fileName},#{url},#{size},#{typeId},#{suffix},#{uploaderId})
    </insert>
</mapper>
